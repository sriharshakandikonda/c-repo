Displaying of first 10 natural numberrs

#include <iostream>

using namespace std;

int main() {
	int i;
	for(i=1;i<=10;i++)
	{
	   cout<<"  " << i;  
	}
   return 0;
	
}

swaping of two numbers

#include <iostream>

using namespace std;

int main() {
	int a=5,b=6;
	a=a+b-(b=a);
	cout << "swap values of a and b are " << a << ',' << b << " " << endl;
   return 0;
	
}

sample struct program 

#include <iostream>

using namespace std;
struct employee
{
    char name[50];
    int id;
};

int main() {
	employee k;
	cout<<"enter details"<<endl;
	cin.get(k.name, 50);
	cin>>k.id;
	cout<<"employee name:"<< k.name;
	cout<<" employee id:"<< k.id;
}
